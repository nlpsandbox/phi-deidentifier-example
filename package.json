{
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "2.4.18",
        "release-it": "^14.10.0"
    },
    "scripts": {
        "generate:server": "openapi-generator-cli generate -g python-flask -o server -i $npm_config_spec",
